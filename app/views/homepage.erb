<head>

  <meta carset=”utf-8>
  <title>Makersbnb</title>

  <link type="text/css" rel="stylesheet" href="css/homepage-style.css"></link>
  <meta name="viewport" content="width=device-width">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

</head>

<body>
  <header>
  <h1>Makersbnb</h1>
  </header>

  <section id='listing-button'>
  <form action="/create-listing" method="get">
    <div id="create-listing-header">Create listing:
    <button id="create-listing">Create a new listing!</button>
  </div>
  </form>
  </section>

  <section id='display-listings'>
  <% @list_of_listings.each do |listing| %>
    <div class= 'listing'>
    <%= listing.list_name %> by <%= listing.username%>
    </div>
  <% end %>
  


  </section>

  <footer>
    Created by the Apocalypses team Asia, Chris, Shaun, & Vish.
  </footer>
</body>
